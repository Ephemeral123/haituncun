/*nav*/
#nav{
	height: 720px;
}
.nav_banner{
	height: 420px;
} 
.nav_sales{
	height: 300px;
	background: burlywood; 
}
.bannerMain{
	width: 100%;
	height: 420px; 
	position: relative;
	overflow: hidden;
	min-width: 1190px;
}
.bannerSlider{
	width: 100%;
	height:420px;
	position: absolute;
	top: 0px;
	left: 0px;
	display: none;
	z-index: 0;
	opacity: 0;
}
.bannerSlider_active{
	width:100%;
	height:420px;
	position: absolute;
	top: 0px;
	left: 0px;
	display: block;
	z-index: 10;
	opacity:1;
}
.bannerSide{
	position:absolute;
	top: 177px;
	left: 50%;
	width: 200px;
	margin-left: 395px;
	overflow: hidden;
	z-index: 20;
	background-color: #fff;
}
.bannerCon{
	position: relative;
	height: 100%;
}
.bannerImg{
	width:770px;
	height:420px;
	position:relative;
	left: 220px;
}
.iconBtn{
	background: url(../img/icon-small.png) no-repeat;
	width: 38px;
	height: 60px;
	position: absolute;
	top: 50%;
	margin-top: -30px;
	z-index: 20;
	text-indent: 10000px;/*内容隐藏*/
}
.btnLeft{
	background-position:-90px 0;
	left:50%;
	transition: all 0.3s;
	margin-left: -415px;
}
.btnRight{
	background-position: -90px -60px;
	right:50%;
	transition: all 0.3s;
	margin-right: -435px;
}
.sideSpectical a{
	display: block;
	border-top: border-top: 1px solid #f1f1f1;;
}
.sideSpectical a img{
	width: 200px;
	height:210px;
	display: block;
}
.nav_sales{
	height:300px;
}
.salesMain{
	float: left;
}
.salesSide{
	float: right;
}
