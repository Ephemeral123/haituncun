<script type="text/javascript">
    jQuery.ajax({
        type: 'get',
        url: ROOT_URL + "o_customer/info/getCustomer",
        dataType: 'jsonp',
        data: 'callback=?',
        success: function (data) {
            if (data.ret == 0) {
                jQuery('.header-topbar-info .login').remove();
                jQuery('.header-topbar-info .login-link').find('a').attr('href', data.loginUrl);
            }
            else {
                jQuery("#isLogin").val(1);
                jQuery('.header-topbar-info .logout').remove();
            }

            jQuery("#customer-service").attr("lim_info", data.live800Info);
            jQuery('.header-topbar-info #customerName').text(data.customerName);
            jQuery('.header-topbar-info').show();
        }
    });
</script>
<input type="hidden" id="isLogin" value="0"/>

