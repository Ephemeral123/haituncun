            if(arguments.length > 0) {
                var newObj = {};
                for(var i = 0; i < arguments.length; i++) {
                    if(Object.prototype.toString.call(arguments[i]) !== '[object Object]') {
                        console.warn("Parameter must be object");
                        return;
